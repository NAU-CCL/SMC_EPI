    "from scipy.stats import nbinom\n",
    "from scipy.special import gamma\n",
    "import numpy as np\n",
    "\n",
    "k = 15\n",
    "n = 14\n",
    "p = 0.59\n",
    "print(nbinom.logpmf(k,n,p))\n",
    "coeff = np.log(gamma(k+n))-np.log(gamma(n+1))-np.log(gamma(k))\n",
    "print(coeff+n*np.log(p)+k*np.log(1-p))\n"
   ]
  }
